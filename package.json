{
  "name": "meshgrouptestapp",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "androidDev": "react-native run-android --variant=devDebug --appIdSuffix dev",
    "androidDevRelease": "npx react-native run-android --variant=devRelease --appIdSuffix dev",
    "androidStg": "react-native run-android --variant=stgDebug --appIdSuffix stg",
    "androidStgRelease": "npx react-native run-android --variant=stgRelease --appIdSuffix stg",
    "androidProd": "react-native run-android --variant=prodDebug",
    "androidProdRelease": "npx react-native run-android --variant=prodRelease",
    "apkDev": "cd android && ./gradlew assembleDevRelease && cd ..",
    "apkStg": "cd android && ./gradlew assembleStgRelease && cd ..",
    "apkProd": "cd android && ./gradlew assembleProdRelease && cd ..",
    "aabDev": "cd android && ./gradlew bundleDevRelease && cd ..",
    "aabStg": "cd android && ./gradlew bundleStgRelease && cd ..",
    "aabProd": "cd android && ./gradlew bundleProdRelease && cd ..",
    "iosDev": "react-native run-ios --configuration=DebugDev --scheme='draft DEV'",
    "iosStg": "react-native run-ios --configuration=DebugStg --scheme='draft STG'",
    "iosProd": "react-native run-ios --configuration=DebugProd --scheme='draft PROD'",
    "start": "react-native start",
    "test": "jest",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "postinstall": "patch-package"
  },
  "dependencies": {
    "@react-native-async-storage/async-storage": "^1.17.12",
    "@react-native-masked-view/masked-view": "^0.2.9",
    "@react-navigation/devtools": "^6.0.10",
    "@react-navigation/native": "^6.1.6",
    "@react-navigation/stack": "^6.3.16",
    "@reduxjs/toolkit": "^1.9.3",
    "axios": "^0.27.2",
    "dayjs": "^1.11.6",
    "i18next": "^22.4.12",
    "mobile-app-debugger": "^1.0.8",
    "patch-package": "^6.5.1",
    "postinstall-postinstall": "^2.1.0",
    "react": "18.1.0",
    "react-i18next": "^12.2.0",
    "react-native": "0.70.7",
    "react-native-gesture-handler": "^2.9.0",
    "react-native-localize": "^2.2.6",
    "react-native-modal": "^13.0.1",
    "react-native-progress": "^5.0.0",
    "react-native-reanimated": "^2.14.4",
    "react-native-safe-area-context": "^4.5.0",
    "react-native-screens": "^3.20.0",
    "react-native-splash-screen": "^3.3.0",
    "react-native-svg": "^13.8.0",
    "react-redux": "^8.0.5",
    "redux": "^4.2.1",
    "redux-flipper": "^2.0.2",
    "redux-persist": "^6.0.0",
    "yup": "^1.0.2"
  },
  "devDependencies": {
    "@babel/core": "^7.12.9",
    "@babel/runtime": "^7.12.5",
    "@react-native-community/eslint-config": "^2.0.0",
    "@tsconfig/react-native": "^2.0.2",
    "@types/jest": "^26.0.23",
    "@types/node": "^18.11.9",
    "@types/react": "^18.0.21",
    "@types/react-native": "^0.70.6",
    "@types/react-test-renderer": "^18.0.0",
    "@typescript-eslint/eslint-plugin": "^5.55.0",
    "@typescript-eslint/parser": "^5.55.0",
    "babel-jest": "^26.6.3",
    "babel-plugin-module-resolver": "^5.0.0",
    "eslint": "^7.32.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-react": "^7.32.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-native": "^4.0.0",
    "eslint-plugin-unused-imports": "^2.0.0",
    "jest": "^26.6.3",
    "metro-react-native-babel-preset": "0.72.3",
    "react-native-flipper": "0.163.0",
    "react-test-renderer": "18.1.0",
    "typescript": "^4.8.3"
  },
  "jest": {
    "preset": "react-native",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  }
}
